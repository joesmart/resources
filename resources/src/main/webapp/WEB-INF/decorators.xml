<?xml version="1.0" encoding="ISO-8859-1"?>

<decorators defaultdir="/WEB-INF/decorators">
    <!-- Any urls that are excluded will never be decorated by Sitemesh -->
    <excludes>
        <pattern>/static/*</pattern>

    </excludes>
    <decorator name="login" page="login.jsp">
        <pattern>/login.jsp</pattern>
        <pattern>/register.jsp</pattern>
        <pattern>/servlet/register</pattern>
    </decorator>
    <decorator name="main" page="decorator.jsp">
       <pattern>/*</pattern>
    </decorator>
    <decorator name="left-panel" page="panel.jsp"/>
    <decorator name="upload" page="upload.jsp">
        <pattern>/upload/*</pattern>
    </decorator>
    <decorator name="other" page="other.jsp">
        <pattern>/other/*</pattern>
    </decorator>
